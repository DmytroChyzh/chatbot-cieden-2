{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_CLERK_PUBLISHABLE_KEY": "@react_app_clerk_publishable_key",
    "CONVEX_URL": "@convex_url"
  }
} 